{
  "name": "bridge-contracts",
  "version": "1.0.0",
  "description": "BRIDGE",
  "main": "index.js",
  "scripts": {
    "prepare": "husky install",
    "lint": "prettier --list-different 'contracts/**/*.sol'",
    "prettier": "prettier --write 'contracts/**/*.sol'",
    "hint": "solhint 'contracts/**/*.sol'",
    "compile": "npx hardhat compile",
    "node": "npx hardhat node",
    "local": "npx hardhat run scripts/deployArchitecture.ts --network localhost",
    "test": "npx hardhat test",
    "coverage": "npx hardhat coverage",
    "bridge": "sh ./scripts/startBridge.sh",
    "deploy::mainnet": "npx hardhat run scripts/deployContract.ts --network mainnet",
    "deploy::kovan": "npx hardhat run scripts/deployContract.ts --network kovan",
    "deploy::ropsten": "npx hardhat run scripts/deployContract.ts --network ropsten",
    "deploy::rinkeby": "npx hardhat run scripts/deployContract.ts --network rinkeby",
    "deploy::goerli": "npx hardhat run scripts/deployContract.ts --network goerli",
    "deploy::gnosis": "npx hardhat run scripts/deployContract.ts --network gnosis",
    "deploy::sokol": "npx hardhat run scripts/deployContract.ts --network sokol",
    "deploy::bsc": "npx hardhat run scripts/deployContract.ts --network bsc",
    "deploy::bsctest": "npx hardhat run scripts/deployContract.ts --network bsctest",
    "deploy::matic": "npx hardhat run scripts/deployContract.ts --network matic",
    "deploy::mumbai": "npx hardhat run scripts/deployContract.ts --network mumbai",
    "deploy::arbitrum": "npx hardhat run scripts/deployContract.ts --network arbitrum",
    "proxy::mainnet": "npx hardhat run scripts/deployProxy.ts --network mainnet",
    "proxy::kovan": "npx hardhat run scripts/deployProxy.ts --network kovan",
    "proxy::ropsten": "npx hardhat run scripts/deployProxy.ts --network ropsten",
    "proxy::rinkeby": "npx hardhat run scripts/deployProxy.ts --network rinkeby",
    "proxy::goerli": "npx hardhat run scripts/deployProxy.ts --network goerli",
    "proxy::gnosis": "npx hardhat run scripts/deployProxy.ts --network gnosis",
    "proxy::sokol": "npx hardhat run scripts/deployProxy.ts --network sokol",
    "proxy::bsc": "npx hardhat run scripts/deployProxy.ts --network bsc",
    "proxy::bsctest": "npx hardhat run scripts/deployProxy.ts --network bsctest",
    "proxy::matic": "npx hardhat run scripts/deployProxy.ts --network matic",
    "proxy::mumbai": "npx hardhat run scripts/deployProxy.ts --network mumbai",
    "proxy::arbitrum": "npx hardhat run scripts/deployProxy.ts --network arbitrum"
  },
